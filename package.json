    "markdown-link-check": "^3.10.0",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "sass": "1.50.1",
    "stylelint": "14.7.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "25.0.0",
    "stylelint-scss": "4.2.0",
    "ts-node-dev": "1.1.8",
    "typescript": "^4.6.3"
